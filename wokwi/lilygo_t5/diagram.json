  "connections": [
    [ "esp:TX", "$serialMonitor:RX", "", [] ],
    [ "esp:RX", "$serialMonitor:TX", "", [] ],
    
    [ "esp:2", "uart1:RX", "green", [ "h0" ] ],
    [ "esp:3", "uart1:TX", "yellow", [ "h0" ] ],
    
    [ "esp:1", "battery_sim:SIG", "orange", [ ] ],
    [ "esp:3V3.1", "battery_sim:VCC", "red", [ "v-85", "h208", "v85" ] ],
    [ "esp:GND.2", "battery_sim:GND", "black", [  ] ],
    
    [ "esp:11", "r_backlight:1", "yellow", [  ] ],
    [ "r_backlight:2", "led_backlight:A", "yellow", [ "v22" ] ],
    [ "esp:GND.4", "led_backlight:C", "black", [  ] ],
    
    [ "esp:46", "lora:CS", "purple", [  ] ],
    [ "esp:14", "lora:SCK", "blue", [  ] ],
    [ "esp:13", "lora:MOSI", "green", [  ] ],
    [ "esp:21", "lora:MISO", "brown", [  ] ],
    [ "esp:47", "lora:BUSY", "orange", [  ] ],
    [ "esp:1", "lora:RESET", "red", [  ] ],
    [ "esp:10", "lora:DIO1", "cyan", [  ] ],
    
    [ "esp:40", "pca:SCL", "blue", [  ] ],
    [ "esp:39", "pca:SDA", "green", [  ] ]
