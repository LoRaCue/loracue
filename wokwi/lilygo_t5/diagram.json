    {
      "type": "wokwi-pushbutton",
      "id": "btn_pca",
      "top": 130,
      "left": 111,
      "rotate": 0,
      "attrs": {
        "color": "red",
        "label": "BUTTON (PCA)"
      }
    },
    {
      "type": "wokwi-potentiometer",
      "id": "battery_sim",
      "top": 0,
      "left": 130,
      "rotate": 0,
      "attrs": {
        "value": "75000",
        "label": "Battery"
      }
    },
    {
      "type": "wokwi-led",
      "id": "led_backlight",
      "top": 240,
      "left": 100,
      "attrs": {
        "color": "white",
        "label": "Backlight"
      }
